# Copy this file to `.env` for local development or testing.
# Do not commit real credentials.

# MySQL
MYSQL_USER=root
MYSQL_PASSWORD=example_password
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_DB=exam_paper_db
MYSQL_ECHO=false

# SQL Server
SQLSERVER_USER=sa
SQLSERVER_PASSWORD=example_password
SQLSERVER_HOST=localhost\\SQLEXPRESS
SQLSERVER_PORT=1433
SQLSERVER_DB=exam_paper_db
SQLSERVER_DRIVER=ODBC Driver 17 for SQL Server
SQLSERVER_SCHEMA=dbo
SQLSERVER_TRUST_SERVER_CERTIFICATE=true
SQLSERVER_ECHO=false

# Oracle
ORACLE_USER=exam_paper_db
ORACLE_PASSWORD=example_password
ORACLE_HOST=localhost
ORACLE_PORT=1521
ORACLE_SERVICE_NAME=ORCLPDB
ORACLE_ECHO=false
